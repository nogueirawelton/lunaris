generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Lead {
  id              String   @id @default(uuid())
  data            Json
  createdAt       DateTime @default(now())
  customerId      String
  customer        Customer @relation(fields: [customerId], references: [id])
}

model Customer {
  id              String   @id @default(uuid())
  email           String   @unique
  phone           String
  name            String
  leads           Lead[]
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
}


